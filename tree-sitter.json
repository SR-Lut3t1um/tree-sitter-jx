{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jx",
      "camelcase": "Jx",
      "title": "Jx Parser",
      "scope": "source.jx",
      "file-types": [
        "jx"
      ],
      "injection-regex": "^jx$",
      "class-name": "TreeSitterJx"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A parser for jx files",
    "authors": [
      {
        "name": "Tobias Liese",
        "email": "mail@tobiasliese.me",
        "url": "https://tobiasliese.me/"
      }
    ],
    "links": {
      "repository": "https://github.com/SR-Lut3t1um/tree-sitter-jx"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}